{
  "name": "next-gen-front-end-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@appbaseio/reactivesearch": "^2.8.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.0-14",
    "@fortawesome/free-solid-svg-icons": "^5.1.0-11",
    "@fortawesome/react-fontawesome": "^0.1.0-11",
    "cookie": "^0.3.1",
    "cross-fetch": "^2.1.0",
    "elasticsearch": "^15.1.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "honeybadger-js": "^0.5.5",
    "jquery": "^3.3.1",
    "node-sass-chokidar": "^1.1.0",
    "npm-run-all": "^4.1.2",
    "openseadragon": "^2.4.0",
    "prop-types": "^15.6.1",
    "react": "^16.0.0",
    "react-collapsible": "^2.3.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.0.0",
    "react-helmet": "^5.2.0",
    "react-notify-toast": "^0.5.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.14",
    "react-share": "^2.2.0",
    "react-sizes": "^1.0.4",
    "react-slidedown": "^2.0.4",
    "react-spinners": "^0.4.7",
    "react-tabs": "^2.2.2",
    "react-test-renderer": "^16.3.1",
    "redux": "^3.7.2",
    "redux-mock-store": "^1.5.1",
    "redux-thunk": "^2.2.0",
    "source-map-explorer": "^1.6.0",
    "swiper": "^4.2.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "proxy": {
    "/solr": {
      "target": "http://localhost:8983"
    }
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "HOST=devbox.library.northwestern.edu PORT=3333 react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "precommit": "lint-staged",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.10.0",
    "husky": "^0.14.3",
    "lint-staged": "^6.1.0",
    "prettier": "^1.13.7"
  }
}
